on join:
  if {bal.%player%} is not set:
    set {bal.%player%} to 0
  
  



on damage: 
  if victim is player:
    add 0.5 to {bal.%attacker%}
    message attacker "Earned 5 credits for killing %victim%"

on command "/shop":
   make console execute command "warp shop %player%"
   send "&7Sending you to &aShop"
